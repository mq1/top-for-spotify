---
import BaseLayout from "../layouts/BaseLayout.astro";
import Obscurity from "../components/Obscurity.tsx";
import Mood from "../components/Mood.tsx";
import Genres from "../components/Genres.tsx";
import Artists from "../components/Artists.tsx";
import Tracks from "../components/Tracks.tsx";
---

<BaseLayout isLoggedIn>
  <main class="max-w-5xl flex flex-col gap-y-8 px-8 mt-32">
    <Obscurity client:load />
    <Mood client:load />
    <Genres client:load />
    <Artists client:load />
    <Tracks client:load />
  </main>
</BaseLayout>
